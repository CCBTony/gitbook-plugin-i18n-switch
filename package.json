{
  "name": "gitbook-plugin-i18n-switch",
  "version": "0.1",
  "description": "国际化页面切换按钮",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ttps://github.com/CCBTony/gitbook-plugin-i18n-switch.git"
  },
  "keywords": [
    "gitbook",
    "plugin",
    "i18n-switch"
  ],
  "author": "TonyGuo",
  "license": "GPL-3.0",
  "engines": {
    "gitbook": ">=3.0.0"
  },
  "gitbook": {
    "properties": {
      "copyright": {
        "type": "string",
        "default": "© All Rights Reserved",
        "title": "你的版权信息",
        "description": "The left-aligned text in the footer."
      },
      "markdown": {
        "type": "boolean",
        "default": false,
        "title": "是否支持Markdown格式信息",
        "description": "True if copyright and update_label are in Markdown, false if plain text or HTML."
      },
      "update_label": {
        "type": "string",
        "default": "updated",
        "title": "文档更新时间标签内容",
        "description": "Text before date."
      },
      "update_format": {
        "type": "string",
        "default": "YYYY-MM-DD HH:mm:ss",
        "title": "时间格式",
        "description": "Moment.js date format."
      }
    }
  }
}
